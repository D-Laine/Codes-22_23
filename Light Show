int red = 9;
int yellow = 10;
int blue = 11;
int yellow2 = 12;
int red2 = 13;

void setup() {
  pinMode(red, OUTPUT);
  pinMode(yellow, OUTPUT);
  pinMode(blue, OUTPUT);
  pinMode(yellow2, OUTPUT);
  pinMode(red2, OUTPUT);
}
  
 void loop() { //10 drums in 20 secs
   /*digitalWrite(red, HIGH);
   digitalWrite(yellow, HIGH);
   digitalWrite(blue, HIGH);
   digitalWrite(yellow2, HIGH);
   digitalWrite(red2, HIGH);*/
   
   //drum 1, 3 notes
   digitalWrite(yellow, HIGH);
   delay(350);
   digitalWrite(blue, HIGH);
   delay(350);
   digitalWrite(yellow2, HIGH);
   delay(350);
   digitalWrite(yellow, LOW);
   digitalWrite(blue, LOW);
   digitalWrite(yellow2, LOW);
   digitalWrite(red, HIGH);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(red, LOW);
   digitalWrite(red2, LOW);
   //5 notes, drum 2
    digitalWrite(yellow, HIGH);
   delay(350);
   digitalWrite(yellow2, HIGH);
   delay(350);
   digitalWrite(blue, HIGH);
   delay(350);
   digitalWrite(yellow, LOW);
   delay(350);
   digitalWrite(yellow2, LOW);
   delay(350);
   digitalWrite(blue, LOW);
   digitalWrite(red, HIGH);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(red, LOW);
   digitalWrite(red2, LOW);
   //5 notes, drum 3
   digitalWrite(yellow2, HIGH);
   delay(350);
   digitalWrite(blue, HIGH);
   delay(350);
   digitalWrite(yellow, HIGH);
   delay(350);
   digitalWrite(yellow2, LOW);
   delay(350);
   digitalWrite(blue, LOW);
   delay(350);
   digitalWrite(yellow, LOW);
   digitalWrite(red, HIGH);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(red, LOW);
   digitalWrite(red2, LOW);
   //5 notes, Drum 4
   digitalWrite(yellow2, HIGH);
   delay(350);
   digitalWrite(yellow, HIGH);
   delay(350);
   digitalWrite(blue, HIGH);
   delay(350);
   digitalWrite(yellow2, LOW);
   delay(350);
   digitalWrite(yellow, LOW);
   delay(350);
   digitalWrite(blue, LOW);
   digitalWrite(red, HIGH);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(red, LOW);
   digitalWrite(red2, LOW);
   //5 notes, Drum 5
   digitalWrite(blue, HIGH);
   delay(350);
   digitalWrite(yellow2, HIGH);
   digitalWrite(yellow, HIGH);
   delay(350);
   digitalWrite(yellow, LOW);
   delay(350);
   digitalWrite(blue, LOW);
   delay(350);
   digitalWrite(yellow2, LOW);
   delay(350);
   digitalWrite(red, HIGH);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(red, LOW);
   digitalWrite(red2, LOW);
   //5 notes, Drum 6
   digitalWrite(yellow, HIGH);
   digitalWrite(yellow2, HIGH);
   digitalWrite(blue, HIGH);
   delay(350);
   digitalWrite(yellow, LOW);
   digitalWrite(yellow2, LOW);
   digitalWrite(blue, LOW);
   delay(350);
   digitalWrite(yellow, HIGH);
   digitalWrite(yellow2, HIGH);
   digitalWrite(blue, HIGH);
   delay(350);
   digitalWrite(yellow, LOW);
   digitalWrite(yellow2, LOW);
   digitalWrite(blue, LOW);
   delay(350);
   digitalWrite(yellow, HIGH);
   digitalWrite(yellow2, HIGH);
   digitalWrite(blue, HIGH);
   delay(350);
   digitalWrite(yellow, LOW);
   digitalWrite(yellow2, LOW);
   digitalWrite(blue, LOW);
   digitalWrite(red, HIGH);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(red, LOW);
   digitalWrite(red2, LOW);
   //5 notes, Drum 7
   digitalWrite(red, HIGH);
   delay(350);
   digitalWrite(yellow, HIGH);
   delay(350);
   digitalWrite(blue, HIGH);
   delay(350);
   digitalWrite(yellow2, HIGH);
   delay(350);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(yellow, LOW);
   digitalWrite(blue, LOW);
   digitalWrite(yellow2, LOW);
   digitalWrite(red, LOW);
   digitalWrite(red2, LOW);
   //5 beats, Drum 8, vocals Start
   digitalWrite(red, HIGH);
   delay(350);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(yellow, HIGH);
   delay(350);
   digitalWrite(yellow2, HIGH);
   delay(350);
   digitalWrite(blue, HIGH);
   delay(390);
   digitalWrite(yellow, LOW);
   digitalWrite(blue, LOW);
   digitalWrite(yellow2, LOW);
   digitalWrite(red, LOW);
   digitalWrite(red2, LOW);
   //5 beats, Drum 9
   digitalWrite(yellow2, HIGH);
   delay(350);
   digitalWrite(red, HIGH);
   delay(350);
   digitalWrite(blue, HIGH);
   delay(350);
   digitalWrite(yellow, HIGH);
   delay(350);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(yellow, LOW);
   digitalWrite(blue, LOW);
   digitalWrite(yellow2, LOW);
   digitalWrite(red, LOW);
   digitalWrite(red2, LOW);
   //5 beats, Drum 10
   digitalWrite(red, HIGH);
   digitalWrite(yellow, HIGH);
   digitalWrite(blue, HIGH);
   digitalWrite(yellow2, HIGH);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(red, LOW);
   digitalWrite(yellow, LOW);
   digitalWrite(blue, LOW);
   digitalWrite(yellow2, LOW);
   digitalWrite(red2, LOW);
   delay(350);
   digitalWrite(red, HIGH);
   digitalWrite(yellow, HIGH);
   digitalWrite(blue, HIGH);
   digitalWrite(yellow2, HIGH);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(red, LOW);
   digitalWrite(yellow, LOW);
   digitalWrite(blue, LOW);
   digitalWrite(yellow2, LOW);
   digitalWrite(red2, LOW);
   delay(350);
   digitalWrite(red, HIGH);
   digitalWrite(yellow, HIGH);
   digitalWrite(blue, HIGH);
   digitalWrite(yellow2, HIGH);
   digitalWrite(red2, HIGH);
   delay(350);
   digitalWrite(red, LOW);
   digitalWrite(yellow, LOW);
   digitalWrite(blue, LOW);
   digitalWrite(yellow2, LOW);
   digitalWrite(red2, LOW);
   //Finish!
 }
